# Environment Variables Template
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env file to git!

# Groq API Key (for Meta LLAMA provider)
# Get your free API key at: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# Oracle Cloud Infrastructure (OCI) Configuration
# Get your compartment ID from OCI Console
OCI_COMPARTMENT_ID=your_oci_compartment_id_here
OCI_BUCKET_NAME=creole-audio-bucket
OCI_NAMESPACE=

# Google Cloud Platform (GCP) Credentials
# Path to your GCP service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/gcp-credentials.json

# Server Configuration
PORT=3001
NODE_ENV=development
